{
  "env": {
    "mocha": true,
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "rules": {
    "import/prefer-default-export": "off",
    "quote-props": ["error", "as-needed"],
    "max-len": ["error", {"code": 140, "ignoreUrls": true}]
  }
}